def calcular_media(lista):
    total = sum(lista)
    media = total / len(lista)
    return media

def verificar_paridade(numero):
    if numero % 2 == 0:
        return "par"
    else:
        return "Ã­mpar"
    
def all(iterable):
    for element in iterable:
        if not element:
            return False
    return True    
